<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/register"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rectangle"
    tools:context=".register">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="555dp"
        android:layout_height="949dp"
        android:layout_marginTop="124dp"
        android:background="@null"
        android:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="126dp"
            android:layout_height="111dp"
            android:background="@null"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.414"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.109"
            app:srcCompat="@drawable/uploadimage" />

        <EditText
            android:id="@+id/editTextText"
            android:layout_width="500dp"
            android:layout_height="73dp"
            android:background="@drawable/text_box"
            android:ems="10"
            android:inputType="text"
            android:text="     NAME"
            android:textColor="@color/white"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="246dp" />

        <EditText
            android:id="@+id/editTextText2"
            android:layout_width="510dp"
            android:layout_height="190dp"
            android:background="@drawable/text_box"
            android:ems="10"
            android:inputType="text"
            android:text="     ADDRESS"
            android:textColor="@color/white"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="-2dp"
            tools:layout_editor_absoluteY="485dp" />

        <EditText
            android:id="@+id/editTextText3"
            android:layout_width="99dp"
            android:layout_height="55dp"
            android:background="@drawable/text_box"
            android:ems="10"
            android:inputType="text"
            android:text="     AGE"
            android:textColor="@color/white"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="-2dp"
            tools:layout_editor_absoluteY="411dp" />

        <EditText
            android:id="@+id/editTextText4"
            android:layout_width="202dp"
            android:layout_height="67dp"
            android:background="@drawable/text_box"
            android:ems="10"
            android:inputType="text"
            android:text="PHONE NUMBER"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="275dp"
            tools:layout_editor_absoluteY="706dp" />

        <EditText
            android:id="@+id/editTextText5"
            android:layout_width="210dp"
            android:layout_height="64dp"
            android:background="@drawable/text_box"
            android:ems="10"
            android:inputType="text"
            android:text="AMOUNT"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="20dp"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="152dp"
            tools:layout_editor_absoluteY="856dp" />

        <EditText
            android:id="@+id/editTextDate"
            android:layout_width="210dp"
            android:layout_height="54dp"
            android:background="@drawable/text_box"
            android:ems="10"
            android:inputType="date"
            android:text="   DATE"
            android:textColor="@color/white"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="169dp"
            tools:layout_editor_absoluteY="336dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="124dp"
        android:layout_height="69dp"
        app:srcCompat="@drawable/profile"
        tools:layout_editor_absoluteX="622dp"
        tools:layout_editor_absoluteY="44dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="249dp"
        android:layout_height="80dp"
        android:text="REGISTER"
        tools:layout_editor_absoluteX="264dp"
        tools:layout_editor_absoluteY="1092dp" />
</androidx.constraintlayout.widget.ConstraintLayout>